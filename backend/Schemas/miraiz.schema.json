          "bloqueo": { "type": ["string", "null"] },
          "precio": { "type": ["number", "null"] },
          "precio_sugerido": { "type": ["number", "null"] },
          "proyectos_id": { "type": ["integer", "null"] },
          "habitaciones": { "type": ["integer", "null"] },
          "baños": { "type": ["number", "null"] },
          "banos": { "type": ["number", "null"] },
          "parqueos": { "type": ["integer", "null"] },
