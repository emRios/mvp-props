          "ano": { "type": ["integer", "null"] },
          "titulo": { "type": ["string", "null"] },
          "descripcion": { "type": ["string", "null"] },
          "detalles": { "type": ["string", "null"] },
          "descripcion_corta": { "type": ["string", "null"] },
