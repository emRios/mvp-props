              "created_at": { "type": ["string", "null"] },
              "updated_at": { "type": ["string", "null"] }
            }
          },
          "imagenes": {
            "type": ["array", "null"],
            "items": {
              "type": "object",
              "required": ["tipo", "url", "formato"],
              "properties": {
                "tipo": { "type": "string" },
                "url": { "type": "string" },
                "formato": { "type": "string" }
              }
            }
          }
        }
      }
    }
  }
}